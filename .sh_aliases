# alias dotfiles='/usr/bin/git --git-dir=$HOME/.dotfiles/ --work-tree=$HOME'

alias ..='cd ..'
alias ...='cd ../../'
alias ....='cd ../../../'
alias .....='cd ../../../../'

alias ll='ls -alF'
alias l='ls -CF'

alias rm='rm -I'
alias tgz='tar -zxvf'

alias gb='git branch -v'
alias gc='git commit'
alias gca='git commit -a'
alias gdf='git diff'
alias gdfm='git diff master'
alias gdfs='git diff --staged'
alias gl='git log --graph --abbrev-commit --oneline --decorate=short'
alias gpl='git pull'
alias gplap='git pull --all -p'
alias gps='git push'
alias gs='git status'

alias dimg='docker image ls'
alias dps='docker ps'
